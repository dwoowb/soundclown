<h1>Sign In</h1>

<form action="<%= session_url %>" method="post">

  <%= auth_token %>

  <label>
    Email
    <br>
    <input type="email" name="user[email]">
  </label>
  <br>

  <label>
    Password
    <br>
    <input type="password" name="user[password]">
  </label>
  <br>

  <input type="submit" value="Sign In">
</form>

<a href="<%= new_user_url %>">Sign Up</a>

