  <section class="track group">
    <article class="track info">
      <h2 class="poster">
        <a href="#/api/users/<%= track.get("poster").id %>">
          <%= track.get("poster").username %>
        </a>
      </h2>
      <a class="artist-track" href="#/api/tracks/<%= track.id %>">
        <%= track.escape("artist") %> - <%= track.escape("title") %>
      </a>
    </article>

    <!-- add nav later, track_show etc -->

  </section>